{
  "manifest_version": 3,
  "name": "Redirect Extension",
  "version": "1.0",
  "description": "Adds a floating button to web pages.",
  "permissions": ["tabs", "activeTab"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "leetcode.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}



